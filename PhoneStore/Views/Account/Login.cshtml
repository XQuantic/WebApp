@model PhoneStore.ViewModels.LoginModel


<div class="loginForm">
    <form asp-action="Login" asp-controller="Account" method="post">
        <a asp-action="Register" asp-controller="Account">Register</a>
        <label asp-for="Email">Enter Email</label>
        <input type="text" asp-for="Email"/>
        <label asp-for="Password">Enter Password</label>
        <input type="password" asp-for="Password"/>
        <input id="inputLoginForm" type="submit" value="Login"/>
    </form>
</div>
